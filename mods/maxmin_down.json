{
  "driver": "{{driver}}{{^driver}}simple{{/driver}}",
  "downlink": [
    {
      "name": "{{name_h}}",
      "label": "{{label_h}}",
      "controll": {
        "type": "number",
        "default": "{{controll_default_h}}",
        "vars": {
          "max": {{controll_max_h}}{{^controll_max_h}}-1{{/controll_max_h}},
          "min": {{controll_min_h}}{{^controll_min_h}}-1{{/controll_min_h}},
          "step": {{controll_step_h}}{{^controll_step_h}}1{{/controll_step_h}}
        }
      },
      "format": {
        "type": "{{format_type_h}}",
        "unit": "{{format_unit_h}}",
        "round": {{format_round_h}}{{^format_round_h}}0{{/format_round_h}}
      }
    },
    {
      "name": "{{name_l}}",
      "label": "{{label_l}}",
      "controll": {
        "type": "number",
        "default": "{{controll_default_l}}",
        "vars": {
          "max": {{controll_max_l}}{{^controll_max_l}}-1{{/controll_max_l}},
          "min": {{controll_min_l}}{{^controll_min_l}}-1{{/controll_min_l}},
          "step": {{controll_step_l}}{{^controll_step_l}}1{{/controll_step_l}}
        }
      },
      "format": {
        "type": "{{format_type_l}}",
        "unit": "{{format_unit_l}}",
        "round": {{format_round_l}}{{^format_round_l}}0{{/format_round_l}}
      }
    }
  ]
}