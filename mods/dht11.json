{
  "driver": "{{driver}}{{^driver}}simple{{/driver}}",
  "uplink": [
    {
      "name": "{{name}}{{^name}}温度{{/name}}",
      "label": "{{label}}{{^label}}T{{/label}}",
      "show": {
        "type": "number",
        "max": {{show_max}}{{^show_max}}-1{{/show_max}},
        "min": {{show_min}}{{^show_min}}-1{{/show_min}}
      },
      "format": {
        "type": "float",
        "unit": "°C",
        "round": {{format_round}}{{^format_round}}0{{/format_round}}
      }
    },
    {
      "name": "{{name_2}}{{^name_2}}湿度{{/name_2}}",
      "label": "{{label_2}}{{^label_2}}H{{/label_2}}",
      "type": "number",
      "show": {
        "max": {{show_max_2}}{{^show_max_2}}-1{{/show_max_2}},
        "min": {{show_min_2}}{{^show_min_2}}-1{{/show_min_2}}
      },
      "format": {
        "type": "float",
        "unit": "%",
        "round": {{format_round_2}}{{^format_round_2}}0{{/format_round_2}}
      }
    }
  ]
}